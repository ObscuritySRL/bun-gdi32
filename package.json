  },
  "exports": {
    ".": "./index.ts"
  },
  "license": "MIT",
  "module": "index.ts",
  "name": "bun-gdi32",
  "peerDependencies": {
    "typescript": "^5"
  },
  "private": false,
  "homepage": "https://github.com/obscuritysrl/bun-gdi32#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/obscuritysrl/bun-gdi32.git"
  },
  "type": "module",
  "version": "1.0.2",
  "main": "./index.ts",
  "keywords": [
    "bun",
    "ffi",
    "win32",
